   "outputs": [],
   "source": [
    "N= int(input(\"Kaç tane asal sayı:  \"))\n",
    "x=2\n",
    "sayac=0\n",
    "\n",
    "\n",
    "while sayac<N:\n",
    "    i=2\n",
    "    while i*i <= x:\n",
    "        if x % i==0:\n",
    "        \n",
    "                break\n",
    "        i+=1\n",
    "    else:\n",
    "        sayac+=1\n",
    "        print(x)\n",
    "                \n",
    "    x+=1"
   ]
  }
